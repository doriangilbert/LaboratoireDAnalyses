<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.univtours.polytech.laboratoiredanalyses_hibernate.controller.PaiementController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label fx:id="labelPaiement" layoutX="246.0" layoutY="26.0" text="Paiement">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Label fx:id="labelLabelMontant" layoutX="146.0" layoutY="90.0" text="Montant à régler :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="labelMontant" layoutX="310.0" layoutY="90.0" text="X €">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="labelTextFieldNumeroCB" layoutX="73.0" layoutY="136.0" text="Numéro de carte bancaire :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldNumeroCB" layoutX="309.0" layoutY="137.0" prefHeight="25.0" prefWidth="291.0" />
            <Label fx:id="labelTextFieldCVV" layoutX="110.0" layoutY="180.0" text="Cryptogramme visuel :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldCVV" layoutX="309.0" layoutY="181.0" />
            <Label fx:id="labelComboBoxMoisExp" layoutX="144.0" layoutY="228.0" text="Mois d'expiration :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ComboBox fx:id="comboBoxMoisExp" layoutX="309.0" layoutY="229.0" prefWidth="150.0" />
            <Label fx:id="labelComboBoxAnneeExp" layoutX="132.0" layoutY="278.0" text="Année d'expiration :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ComboBox fx:id="comboBoxAnneeExp" layoutX="309.0" layoutY="279.0" prefWidth="150.0" />
            <Button fx:id="buttonRetourAccueil" layoutX="70.0" layoutY="339.0" mnemonicParsing="false" onAction="#handleButtonRetourAccueil" text="Retourner à l'accueil" />
            <Button fx:id="buttonPayer" layoutX="468.0" layoutY="332.0" mnemonicParsing="false" onAction="#handleButtonPayer" text="Payer la visite">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
      </children>
    </AnchorPane>
  </children>
</VBox>
