<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.univtours.polytech.laboratoiredanalyses_hibernate.controller.ReservationController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="labelReservation" layoutX="228.0" layoutY="32.0" text="Réservation">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Label fx:id="labelComboBoxAnalyse" layoutX="59.0" layoutY="119.0" text="Sélectionner l'analyse à réserver :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ComboBox fx:id="comboBoxAnalyse" layoutX="338.0" layoutY="120.0" onAction="#handleComboBoxAnalyse" prefHeight="25.0" prefWidth="262.0" promptText="Cliquer pour choisir une analyse" />
            <Label fx:id="labelLabelDate" layoutX="59.0" layoutY="187.0" text="Date de la première visite disponible : ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="labelDate" layoutX="380.0" layoutY="187.0" text="&lt;Choisir une analyse&gt;">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="labelLabelHeure" layoutX="59.0" layoutY="255.0" text="Heure de la première visite disponible :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="labelHeure" layoutX="380.0" layoutY="255.0" text="&lt;Choisir une analyse&gt;">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="buttonRetourAccueil" layoutX="59.0" layoutY="328.0" mnemonicParsing="false" onAction="#handleButtonRetourAccueil" text="Retourner à l'accueil" />
            <Button fx:id="buttonReserver" disable="true" layoutX="402.0" layoutY="321.0" mnemonicParsing="false" onAction="#handleButtonReserver" text="Réserver la visite">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
    </AnchorPane>
  </children>
</VBox>
